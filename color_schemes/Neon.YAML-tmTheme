%YAML 1.2
---

constant:
  name: Numbers
  scope: constant.numeric
  settings:
    foreground: '#FF0604'
  name: Language Constant
  scope: constant.language
  settings:
    fontStyle: bold italic
    foreground: '#FF1308'
  name: Constant.character.entity
  scope: constant.character.entity, punctuation.definition.constant
  settings:
    fontStyle: italic
    foreground: '#FF07A0'

entity:
  name: Entity-Other
  scope: entity.other
  settings:
    foreground: '#04FFD7'
  name: Inherited class
  scope: entity.other.inherited-class
  settings:
    foreground: '#80FCFF'

invalid:
  name: Invalid
  scope: invalid
  settings:
    background: '#FF1E00'
    fontStyle: bold italic
    foreground: '#FFFFFF'

keyword:
  name: Keyword
  scope: keyword, storage.control.matlab
  settings:
    foreground: '#0B93FF'
  name: Keyword other
  scope: keyword.other
  settings:
    foreground: '#FF026A'
  name: Operator
  scope: keyword.operator -keyword.operator.dereference
  settings:
    foreground: '#A7A3FF'
  name: Comparison operator
  scope: keyword.operator.comparison
  settings:
    foreground: '#48FFC2'
  name: Assignment operator
  scope: keyword.operator.assignment
  settings:
    foreground: '#FF6106'
  name: Augmented assignment operator
  scope: keyword.operator.assignment.augmented
  settings:
    foreground: '#FFBBF4'

meta:
  name: Dict key
  scope: meta.structure.dictionary meta.structure.dictionary.key, constant.other.object.key
    string
  settings:
    fontStyle: bold
    foreground: '#A9FF98'
  name: Dict Value
  scope: meta.structure.dictionary.value
  settings:
    foreground: '#7DC5F9'
  name: Item-access arguments
  scope: meta.item-access.arguments
  settings:
    foreground: '#11BD7C'
  name: Class
  scope: meta.class.identifier, entity.name.type.class, support.class, variable.other.class,
    entity.name.class
  settings:
    foreground: '#CFFF01'
  name: Method
  scope: meta.method.identifier, meta.method-call, meta.method.declaration
  settings:
    foreground: '#13FFA3'
  name: Function definition
  scope: meta.function entity.name.function, entity.name.function.abp, meta.prototype.function.js,
    entity.name.function.js
  settings:
    background: '#07019A'
    foreground: '#0AFF04'
  name: Function call
  scope: meta.function-call, support.function.name, meta.function-call punctuation.definition.parameters,
    meta.function-call.method.without-arguments.js -meta.group.braces.round.function.arguments.js
  settings:
    foreground: '#10FF02'
  name: Function arguments
  scope: meta.function-call.arguments
  settings:
    fontStyle: italic
    foreground: '#2BB71D'

storage:
  name: Storage type
  scope: storage.type
  settings:
    foreground: '#FF4101'
  name: Storage modifier
  scope: storage.modifier
  settings:
    foreground: '#276AFF'

string:
  name: String
  scope: string -string.unquoted.old-plist -string.unquoted.heredoc, string.unquoted.heredoc
    string
  settings:
    foreground: '#FFDF02'
  name: String Quotes
  scope: string.quoted punctuation.definition.string.begin, string.quoted punctuation.definition.string.end,
    punctuation.definition.string
  settings:
    fontStyle: italic
    foreground: '#FF07A2'
  name: String constant
  scope: string constant
  settings:
    fontStyle: italic
    foreground: '#FF087B'
  name: String embedded-source
  scope: string.quoted source
  settings:
    background: '#414141'
    foreground: '#EBFF59'
  name: String.regexp
  scope: string.regexp
  settings:
    foreground: '#FFE4A6'
  name: String variable
  scope: string variable
  settings:
    foreground: '#EF9975'

support:
  name: Support.constant
  scope: support.constant
  settings:
    fontStyle: italic
    foreground: '#EB939A'
  name: Exception
  scope: support.type.exception
  settings:
    background: '#800F00'
    foreground: '#F8F8F8'
  name: Support function
  scope: support.function
  settings:
    foreground: '#E5A5FF'
  name: Builtin function
  scope: support.function.builtin
  settings:
    fontStyle: italic
    foreground: '#E0A1FF'
  name: Support.type
  scope: support.type
  settings:
    foreground: '#FF1190'
  name: Support variable
  scope: support.variable
  settings:
    foreground: '#0A4AFF'

variable:
  name: Variable
  scope: variable, punctuation.definition.variable
  settings:
    foreground: '#FF25D9'
  name: Other Variable
  scope: variable.other, meta.variable.other.valid
  settings:
    foreground: '#D285CC'
  name: Language Variable
  scope: variable.language, variable.parameter.function.language
  settings:
    fontStyle: italic
    foreground: '#0AEDFF'
  name: Function definition parameters
  scope: variable.parameter.function
  settings:
    fontStyle: italic
    foreground: '#FFF57F'



